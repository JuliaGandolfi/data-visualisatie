void draw_array(Song[][] arr, int x_offset, int y_offset) {
  // doe iets met arr
  
  int aantal_vorig_getekende = 0;
  int vorig_jaar = 0;
  int verschil_jaar = 0;
  for (int j = 0; j < arr.length; j++) {
        Song[] subArray = arr[j];
        
        //Song[] ....

    int aantal_titels_per_jaar = subArray.length;
      
    int extra_marge = aantal_vorig_getekende * 20;

    for (int i = 0; i < subArray.length; i++) {
      println(subArray[i]);
      
      // Song thisSong = subArray[i]; 
      
      // println(thisSong.titel);


      y = i * 20 + extra_marge + y_offset;
      x = j * 20 + x_offset;
      
      // thisSong.x = x;
      // thisSong.y = y;

      pushMatrix();
      translate(x, y);
      rotate(-PI / 4);
      //teken vanuit 0,0
      fill(0);
      text(subArray[i], 0, 0); //eerste error
      popMatrix();
    } 
   if (aantal_titels_per_jaar > 0){ 
     verschil_jaar = j - vorig_jaar;
     vorig_jaar = j;
    aantal_vorig_getekende += aantal_titels_per_jaar - verschil_jaar;
   }
  }//einde all_1
}

